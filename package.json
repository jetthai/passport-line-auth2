{
  "name": "@jetthai/passport-line-auth2",
  "version": "2.0.9",
  "description": "Line Authentication Strategy for Passport",
  "main": "./dist",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "prepare": "npm run build",
    "start": "node ./example/app.js",
    "test": "ava --verbose",
    "coverage": "rm -rf .nyc_output && nyc ava",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "release": "standard-version && git push --follow-tags origin master && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jetthai/passport-line-auth2.git"
  },
  "keywords": [
    "line",
    "passport",
    "identity",
    "auth",
    "authentication"
  ],
  "author": "Jetthai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jetthai/passport-line-auth2/issues"
  },
  "homepage": "https://github.com/jetthai/passport-line-auth2#readme",
  "engines": {
    "node": ">= 0.4.0",
    "yarn": ">= 0.8.0"
  },
  "dependencies": {
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.9",
    "@types/passport-oauth2": "^1.4.17",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "ava": "^6.2.0",
    "body-parser": "^1.20.3",
    "codecov": "^3.8.3",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unicorn": "^57.0.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "globals": "^16.0.0",
    "jsonwebtoken": "^9.0.2",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "pug": "^3.0.3",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "packageManager": "yarn@3.3.0"
}
